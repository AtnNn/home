(load-file "~/emacs/init.el")
(custom-set-variables
 ;; custom-set-variables was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(blink-cursor-mode nil)
 '(column-number-mode t)
 '(compilation-auto-jump-to-first-error t)
 '(default-input-method "rfc1345")
 '(display-battery-mode t)
 '(display-time-mode t)
 '(ecb-options-version "2.32")
 '(fringe-mode 0 nil (fringe))
 '(haskell-program-name "bash")
 '(inhibit-startup-screen t)
 '(inverse-video t)
 '(js2-auto-indent-flag nil)
 '(js2-bounce-indent-flag nil)
 '(js2-electric-keys (quote nil))
 '(js2-idle-timer-delay 1.0)
 '(js2-mirror-mode nil)
 '(menu-bar-mode t)
 '(mouse-autoselect-window t)
 '(org-agenda-files (quote ("~/TODO")))
 '(save-place t nil (saveplace))
 '(savehist-autosave-interval 30)
 '(savehist-mode t nil (savehist))
 '(scroll-conservatively 20)
 '(show-paren-mode t)
 '(size-indication-mode t)
 '(sqlplus-session-cache-dir "~/.sqlplus-sessions/")
 '(tool-bar-mode nil)
 '(use-file-dialog nil)
 '(w3m-key-binding (quote info)))
(custom-set-faces
 ;; custom-set-faces was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(mode-line ((((class color) (min-colors 88)) (:background "grey75" :foreground "black" :inverse-video t :height 0.7))))
 '(mode-line-inactive ((default (:inherit mode-line)) (((class color) (min-colors 88) (background light)) (:background "white" :foreground "grey20" :inverse-video nil :overline t :underline t)))))
