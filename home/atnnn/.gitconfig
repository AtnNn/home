[core]
  excludesfile = "/home/atnnn/.config/git/excludes.default"
[color]
    ui = auto
[user]
  name = Etienne Laurin
  email = etienne@atnnn.com
[log]
  abbrevCommit = true
  decorate = short
[push]
  default = upstream
[rerere]
  enabled = true
[branch]
  autosetupmerge = false
[merge]
  conflictstyle = diff3
[mergetool "emacs"]
  cmd = emacs -nw -Q --no-desktop --eval '(ediff-merge-files-with-ancestor \"'$LOCAL'\" \"'$REMOTE'\" \"'$BASE'\" nil \"'$MERGED'\")'
