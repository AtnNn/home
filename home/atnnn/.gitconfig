[core]
	excludesfile = "/home/atnnn/.config/git/excludes.default"
[color]
    ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
	whitespace = red reverse
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[user]
	name = Etienne Laurin
	email = etienne@atnnn.com
[format]
	pretty = oneline
[log]
	abbrevCommit = true
	decorate = short
[push]
	default = upstream
[rerere]
	enabled = true
[branch]
	autosetupmerge = false
[merge]
	conflictstyle = diff3
[alias]
	repo-config = config
